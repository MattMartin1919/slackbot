{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "object-curly-spacing": [2, "always"],
        "indent": [2, 2, {"SwitchCase": 1}],
        "space-before-function-paren": 0
    }
}
